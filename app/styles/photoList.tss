}

'#photoDetailScrollView': {
  width: Ti.UI.FILL,
  height: Ti.UI.FILL,
  maxZoomScale: 1
}

'#photoDetail': {
  width: Ti.UI.SIZE,
  height: Ti.UI.SIZE,
  backgroundColor: '#FFFFFF'
}

